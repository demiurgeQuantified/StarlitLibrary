{
    "$schema": "https://raw.githubusercontent.com/EmmyLuaLs/emmylua-analyzer-rust/refs/heads/main/crates/emmylua_code_analysis/resources/schema.json",
    "workspace": {
        "enableReindex": false,
        "encoding": "utf-8",
        "ignoreDir": [
            "Contents/mods/StarlitLibrary/media",
            "tests",
            "Contents/mods/StarlitLibrary/42",
            "Contents/mods/StarlitLibrary/42.12"
        ],
        "ignoreGlobs": [],
        "library": [
            "$PZ_UMBRELLA"
        ],
        "moduleMap": [],
